@model List<Billing>



<!--  BEGIN CONTENT AREA  -->
<div id="content" class="main-content">
    <div class="layout-px-spacing">

        <div class="middle-content container-xxl p-0">
            <div class="row layout-spacing">
                <div class="col-lg-12">
                    <div class="statbox widget box box-shadow">
                        <div class="widget-content widget-content-area">
                            <table id="style-3" class="table style-3 dt-table-hover">
                                <thead>
                                    <tr>
                                        <th class="text-center">Billing At</th>

                                        <th class="text-center"> Bill No. </th>
                                        
                                        <th>Customer Name</th>
                                        <th>Address</th>
                                        <th>Mobile No.</th>
                                        <th>Policy</th>
                                        <th>Amount</th>
                                       

                                        <th class="text-center dt-no-sorting">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                     @foreach (var bill in Model) 
                                    {

                                        <tr>
                                            <td>@bill.BillingAt</td>
                                            <td> @bill.BillNo  </td>
                                           
                                            <td>@bill.CustomerName</td>
                                            <td>@bill.CustomerAddress</td>
                                            <td>@bill.CustomerPhoneNumber</td>
                                            <td>@bill.VehiclePolicyType</td>
                                            <td>@bill.Amount $</td>
                                       
                                                                                      
                                            <td class="text-center">
                                                <a asp-controller="Billing" asp-action="View" asp-route-id="@bill.Id" class="action-btn btn-edit bs-tooltip me-2" data-toggle="tooltip" data-placement="top" title="View">
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-eye"><path d="M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z"></path><circle cx="12" cy="12" r="3"></circle></svg>
                                                </a>
                                            </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!--  END CONTENT AREA  -->
</div>

<!--  END CONTENT AREA  -->
@section Scripts {
    @{
        <partial name="_TableRecordScript" />

    }
}

